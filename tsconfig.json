{
  "compilerOptions": {
    "allowJs": true,
    "module": "commonjs",
    "target": "es6",
    "lib": ["es2018", "dom"],
    "noImplicitAny": false,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "types": ["./index.d.ts"],
    "noEmit": true,
    "forceConsistentCasingInFileNames": true
  }
}
